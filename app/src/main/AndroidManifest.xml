    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />

    <application
        android:name="vn.vietmap.services.android.navigation.testapp.NavigationApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="vn.vietmap.services.android.navigation.testapp.CustomUINavigationMap"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name="vn.vietmap.services.android.navigation.testapp.sample_3"
            android:exported="false" />
        <activity
            android:name="vn.vietmap.services.android.navigation.testapp.NavigationLauncherActivity"
            android:exported="false" />
        <activity
            android:name="vn.vietmap.services.android.navigation.testapp.sample_1"
